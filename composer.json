{
    "name": "kora/debug",
    "type": "library",
    "description": "Debug for the Kora Framework.",
    "homepage": "https://github.com/koraphp/debug",
    "license": "MIT",
    "authors": [
        {
            "name": "Uriel Wilson",
            "email": "uriel@koraphp.com",
            "homepage": "https://koraphp.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
		"filp/whoops": "^2.15",
        "symfony/var-dumper": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Kora\\Debug\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Debug\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
